{
  "id": "app",
  "backoffFactor": 2.0,
  "backoffSeconds": 30,
  "container": {
    "docker": {
      "forcePullImage": true,
      "image": "0x06065a/joker2016-sample-api:0.0.5",
      "network": "BRIDGE",
      "parameters": [
        {
          "key": "memory",
          "value": "768m"
        }
      ],
      "portMappings": [
        {
          "containerPort": 8080
        }
      ],
      "privileged": false
    },
    "type": "DOCKER"
  },
  "env": {
    "SERVICE_NAME": "app"
  },
  "cpus": 0.1,
  "healthChecks": [
    {
      "gracePeriodSeconds": 300,
      "ignoreHttp1xx": false,
      "intervalSeconds": 3,
      "maxConsecutiveFailures": 3,
      "path": "/health",
      "portIndex": 0,
      "protocol": "HTTP",
      "timeoutSeconds": 10
    }
  ],
  "instances": 1,
  "maxLaunchDelaySeconds": 86400,
  "mem": 512.0,
  "upgradeStrategy": {
    "maximumOverCapacity": 1.0,
    "minimumHealthCapacity": 0.5
  }
}
