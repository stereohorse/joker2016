[
  {
    "id": "app",
    "backoffFactor": 2.0,
    "backoffSeconds": 30,
    "container": {
      "docker": {
        "forcePullImage": true,
        "image": "0x06065a/joker2016-sample-api:0.0.7",
        "network": "BRIDGE",
        "parameters": [
          {
            "key": "memory",
            "value": "450m"
          }
        ],
        "portMappings": [
          {
            "containerPort": 8080
          }
        ],
        "privileged": false
      },
      "type": "DOCKER"
    },
    "env": {
      "SERVICE_NAME": "app",
      "START_DELAY_SECS": "10"
    },
    "cpus": 0.1,
    "healthChecks": [
      {
        "gracePeriodSeconds": 300,
        "ignoreHttp1xx": false,
        "intervalSeconds": 3,
        "maxConsecutiveFailures": 3,
        "path": "/health",
        "portIndex": 0,
        "protocol": "HTTP",
        "timeoutSeconds": 10
      }
    ],
    "labels": {
      "label1": "5"
    },
    "instances": 2,
    "maxLaunchDelaySeconds": 86400,
    "mem": 512.0,
    "upgradeStrategy": {
      "maximumOverCapacity": 0.5,
      "minimumHealthCapacity": 1.0
    }
  }
]
